{
  "extends": "@parcel/config-default",
  "compressors": {
    "*.{html,css,js,svg,map}": [
      "...",
      "@parcel/compressor-gzip",
      "@parcel/compressor-brotli"
    ]
  },
  "transformers": {
    "icons/*.svg": [
      "@company/parcel-transformer-svg-icons",
      "..."
    ],
    "*.svg": [
      "@parcel/transformer-svg"
    ]
  }
}
